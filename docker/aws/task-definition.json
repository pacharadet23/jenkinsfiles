{
    "family": "devops-taskdefinitions",
    "containerDefinitions": [
        {
            "image": "980259306743.dkr.ecr.ap-southeast-1.amazonaws.com/%REPO%",
            "name": "devops-container",
            "essential": true,
            "cpu": 10,
            "memoryReservation": 128,
            "portMappings": [
                {
                    "containerPort": 80,
                    "hostPort": 80,
                    "protocol": "tcp"
                },
                {
                    "containerPort": 443,
                    "hostPort": 443,
                    "protocol": "tcp"
                }
            ]
        }
    ]
}
